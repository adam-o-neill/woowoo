{
  "expo": {
    "name": "woowoo app",
    "slug": "woowoo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "woowooapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.adamoneill.woowooapp",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDisplayName": "woowooapp",
        "LSApplicationQueriesSchemes": ["itms-apps"],
        "UIBackgroundModes": ["fetch"],
        "NSCalendarsUsageDescription": "This app needs access to calendar to show astrological forecasts for specific dates."
      }
    },
    "description": "Your daily astrological guide with planetary positions and personalized forecasts.",
    "primaryColor": "#000000",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "extra": {
      "eas": {
        "projectId": "32b3a395-3064-43a0-a497-78ffe43a70bb"
      }
    },
    "plugins": ["expo-router"]
  }
}
